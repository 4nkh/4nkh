                <%= yield(:head) %>
                <script type="text/javascript">
                        //<![CDATA[
                        function pageLoad() {
                                // Creates rollover images for clients logos
                                var aImages = document.getElementById("clientsList").getElementsByTagName("img");
                                var imagesList = new Array();
                                
                                for ( var i = 0; i < document.getElementById("clientsList").getElementsByTagName("img").length; i++ ) {
                                        imagesList.push(document.getElementById("clientsList").getElementsByTagName("img")[i]);
                                }
                                
                                for ( var i = 0; i < imagesList.length; i++ ) {
                                        var clone = imagesList[i].cloneNode(true);
                                        clone.className = "hover";
                                        imagesList[i].parentNode.appendChild(clone);
                                }
                                
                                createTransitionDelay(document.getElementById("clientsList"));
                                
                                addEvt(document.getElementById("clientsButton"), "click", clientsButtonClick);
                        }
                        
                        addEvt(window, "load", pageLoad);
                        
                        
                        function clientsButtonClick(evt) {
                                var clientsButton = document.getElementById("clientsButton");
                                var clientsElement = document.getElementById("clients");
                                
                                // Hide the element
                                if ( (/shown\s/).test(clientsElement.className) ) {
                                        clientsButton.className = clientsButton.className.replace(/doHide\s/g, "");
                                        clientsElement.className = clientsElement.className.replace(/shown\s/gi, "");
                                        clientsElement.style.height = "0";
                                }
                                
                                // Shows the element
                                else {
                                        if (!(/doHide!\s/).test(clientsButton.className))
                                                clientsButton.className = "doHide " + clientsButton.className;
                                        clientsElement.className = "shown " + clientsElement.className;
                                        clientsElement.style.height = String(clientsElement.getElementsByTagName("div")[0].offsetHeight) + "px";
                                        
                                        setTimeout(function() { scrollToElement(document.getElementById("clientsButton")); }, 200);
                                }
                        }
                        //]]>
                </script>

