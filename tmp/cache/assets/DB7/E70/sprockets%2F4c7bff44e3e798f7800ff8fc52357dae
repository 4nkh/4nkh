	/* Social Media : Fade In */
	/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
	@-webkit-keyframes SocialMediaFadeIn {
		0%   { opacity: 0; width: 0; height: 0; }
		1%   { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 1; width: 100%; height: 100%; }
	}
	@-moz-keyframes SocialMediaFadeIn {
		0%   { opacity: 0; width: 0; height: 0; }
		1%   { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 1; width: 100%; height: 100%; }
	}
	@-ms-keyframes SocialMediaFadeIn {
		0%   { opacity: 0; width: 0; height: 0; }
		1%   { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 1; width: 100%; height: 100%; }
	}
	@-o-keyframes SocialMediaFadeIn {
		0%   { opacity: 0; width: 0; height: 0; }
		1%   { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 1; width: 100%; height: 100%; }
	}
	@keyframes SocialMediaFadeIn {
		0%   { opacity: 0; width: 0; height: 0; }
		1%   { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 1; width: 100%; height: 100%; }
	}
	
	
	
	
	/* Social Media : Fade Out */
	/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
	@-webkit-keyframes SocialMediaFadeOut {
		0%   { opacity: 1; width: 100%; height: 100%; }
		99%  { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 0; width: 0; height: 0; }
	}
	@-moz-keyframes SocialMediaFadeOut {
		0%   { opacity: 1; width: 100%; height: 100%; }
		99%  { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 0; width: 0; height: 0; }
	}
	@-ms-keyframes SocialMediaFadeOut {
		0%   { opacity: 1; width: 100%; height: 100%; }
		99%  { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 0; width: 0; height: 0; }
	}
	@-o-keyframes SocialMediaFadeOut {
		0%   { opacity: 1; width: 100%; height: 100%; }
		99%  { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 0; width: 0; height: 0; }
	}
	@keyframes SocialMediaFadeOut {
		0%   { opacity: 1; width: 100%; height: 100%; }
		99%  { opacity: 0; width: 100%; height: 100%; }
		100% { opacity: 0; width: 0; height: 0; }
	}
