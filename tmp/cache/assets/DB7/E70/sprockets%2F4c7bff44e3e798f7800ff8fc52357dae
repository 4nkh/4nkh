/* Animations */
/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
	
	/* Creations Slider : Show */
	/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
	@-webkit-keyframes SliderShow {
		0%   { opacity: 0; -webkit-transform: scale3d(.95,.95,1); }
		100% { opacity: 1; -webkit-transform: scale3d(1,1,1); }
	}
	@-moz-keyframes SliderShow {
		0%   { opacity: 0; -moz-transform: scale(.95,.95); }
		100% { opacity: 1; -moz-transform: scale(1,1); }
	}
	@-ms-keyframes SliderShow {
		0%   { opacity: 0; -ms-transform: scale(.5,.5); }
		100% { opacity: 1; -ms-transform: scale(1,1); }
	}
	@-o-keyframes SliderShow {
		0%   { opacity: 0; -o-transform: scale(.95,.95); }
		100% { opacity: 1; -o-transform: scale(1,1); }
	}
	@keyframes SliderShow {
		0%   { opacity: 0; transform: scale(.95,.95); }
		100% { opacity: 1; transform: scale(1,1); }
	}
	
	
	
	
	/* Creations Slider : Hide */
	/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
	@-webkit-keyframes SliderHide {
		0%   { width: 100%; opacity: 1; -webkit-transform: scale3d(1,1,1); }
		99%  { width: 100%; opacity: 0; -webkit-transform: scale3d(.95,.95,1); }
		100% { width: 0; }
	}
	@-moz-keyframes SliderHide {
		0%   { width: 1004px; opacity: 1; -moz-transform: scale(1,1); }
		99%  { width: 1004px; opacity: 0; -moz-transform: scale(.95,.95); }
		100% { width: 0; }
	}
	@-ms-keyframes SliderHide {
		0%   { width: 1004px; opacity: 1; -ms-transform: scale(1,1); }
		99%  { width: 1004px; opacity: 0; -ms-transform: scale(.95,.95); }
		100% { width: 0; }
	}
	@-o-keyframes SliderHide {
		0%   { width: 1004px; opacity: 1; -o-transform: scale(1,1); }
		99%  { width: 1004px; opacity: 0; -o-transform: scale(.95,.95); }
		100% { width: 0; }
	}
	@keyframes SliderHide {
		0%   { width: 100%; opacity: 1; transform: scale(1,1); }
		99%  { width: 100%; opacity: 0; transform: scale(.95,.95); }
		100% { width: 0; }
	}
	
	
	
	
	/* Creations List : Show */
	/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
